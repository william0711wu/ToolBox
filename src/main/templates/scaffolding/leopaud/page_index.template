</content>

<${''}%--使用模板-页面自定义js--%>
<content tag="pagerCustomJs">
  <script type="application/javascript">
   \$(document).ready(function () {
    //define in scaffolding.js
    scaffolding = \$("#scaffolding").scaffolding({queryUrl:"/example/list",
        newUrl:"/example/new",
        editUrl:"/example/edit",
        delUrl:"/example/del",
        showUrl:"/example/show"})
    scaffolding.query()

});
</script>
